{
  "name": "swpm",
  "version": "2.3.0",
  "engines": {
    "bun": ">=1.0.0"
  },
  "description": "Switch Package Manager",
  "main": "./src/cli/swpm.js",
  "bin": {
    "swpm": "./src/cli/swpm.js",
    "swpx": "./src/cli/swpx.js",
    "si": "./src/alias/si.js",
    "sif": "./src/alias/sif.js",
    "sa": "./src/alias/sa.js",
    "sae": "./src/alias/sae.js",
    "sad": "./src/alias/sad.js",
    "sade": "./src/alias/sade.js",
    "sag": "./src/alias/sag.js",
    "srm": "./src/alias/srm.js",
    "srg": "./src/alias/srg.js",
    "sup": "./src/alias/sup.js",
    "sug": "./src/alias/sug.js",
    "sui": "./src/alias/sui.js",
    "soe": "./src/alias/soe.js",
    "sgr": "./src/alias/sgr.js",
    "sgb": "./src/alias/sgb.js",
    "sgp": "./src/alias/sgp.js",
    "sgm": "./src/alias/sgm.js",
    "sgd": "./src/alias/sgd.js",
    "soc": "./src/alias/soc.js",
    "son": "./src/alias/son.js",
    "scr": "./src/alias/scr.js",
    "scn": "./src/alias/scn.js",
    "scl": "./src/alias/scl.js",
    "scb": "./src/alias/scb.js",
    "scd": "./src/alias/scd.js",
    "scc": "./src/alias/scc.js",
    "sca": "./src/alias/sca.js",
    "scf": "./src/alias/scf.js",
    "sci": "./src/alias/sci.js",
    "sr": "./src/alias/sr.js",
    "sp": "./src/alias/sp.js",
    "spn": "./src/alias/spn.js",
    "spy": "./src/alias/spy.js",
    "spyb": "./src/alias/spyb.js",
    "spp": "./src/alias/spp.js",
    "spb": "./src/alias/spb.js",
    "sx": "./src/alias/sx.js"
  },
  "scripts": {
    "prebuild": "rm -rf bin/",
    "build": "bun run build.ts",
    "postbuild": "cp package*.json .npmignore README.md LICENSE.md CHANGELOG.md bin && (cd bin && bun install --frozen-lockfile --production --ignore-scripts)",
    "pack": "bun pack bin/ --pack-destination=\"bin/\"",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --fix --ext .js,.ts",
    "test": "bun test",
    "test:w": "bun test --watch",
    "test:c": "bun test --coverage",
    "test:wc": "bun test --watch --coverage",
    "ts:check": "tsc --noEmit",
    "ts:diagnostics": "tsc --extendedDiagnostics",
    "ts:trace": "tsc --traceResolution"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deinsoftware/swpm.git"
  },
  "keywords": [
    "swpm",
    "npm",
    "yarn",
    "pnpm",
    "bun",
    "cli",
    "command-line",
    "package-manager",
    "javascript",
    "typescript"
  ],
  "author": "deinsoftware",
  "contributors": [
    "equiman"
  ],
  "sponsor": {
    "url": "https://github.com/sponsors/deinsoftware"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deinsoftware/swpm/issues"
  },
  "homepage": "https://github.com/deinsoftware/swpm#readme",
  "type": "module",
  "dependencies": {
    "chalk": "5.3.0",
    "command-exists": "1.2.9",
    "common-tags": "1.8.2",
    "find-up": "6.3.0",
    "open": "9.1.0",
    "prompts": "2.4.2",
    "semver": "7.5.4",
    "spinnies": "0.5.1",
    "update-notifier": "6.0.2",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@types/command-exists": "1.2.1",
    "@types/common-tags": "1.8.2",
    "@types/prompts": "2.4.5",
    "@types/semver": "7.5.3",
    "@types/spinnies": "0.5.1",
    "@types/update-notifier": "6.0.5",
    "@types/yargs": "17.0.26",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "bun-types": "1.0.3",
    "eslint": "8.50.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-n": "16.1.0",
    "eslint-plugin-promise": "6.1.1",
    "type-fest": "4.3.3",
    "typescript": "5.2.2"
  },
  "packageManager": "bun@1.0.3",
  "swpm": "bun"
}