{
  "name": "swpm",
  "version": "2.0.0-beta-1",
  "engines": {
    "node": ">=16"
  },
  "description": "Switch Package Manager",
  "main": "./src/cli/swpm.js",
  "bin": {
    "swpm": "./src/cli/swpm.js",
    "swpx": "./src/cli/swpx.js",
    "si": "./src/alias/si.js",
    "sif": "./src/alias/sif.js",
    "sa": "./src/alias/sa.js",
    "sae": "./src/alias/sae.js",
    "sad": "./src/alias/sad.js",
    "sade": "./src/alias/sade.js",
    "sag": "./src/alias/sag.js",
    "srm": "./src/alias/srm.js",
    "srg": "./src/alias/srg.js",
    "sup": "./src/alias/sup.js",
    "sug": "./src/alias/sug.js",
    "sui": "./src/alias/sui.js",
    "scr": "./src/alias/scr.js",
    "scn": "./src/alias/scn.js",
    "scl": "./src/alias/scl.js",
    "scb": "./src/alias/scb.js",
    "scd": "./src/alias/scd.js",
    "scc": "./src/alias/scc.js",
    "sca": "./src/alias/sca.js",
    "scf": "./src/alias/scf.js",
    "sci": "./src/alias/sci.js",
    "sr": "./src/alias/sr.js",
    "sp": "./src/alias/sp.js",
    "spn": "./src/alias/spn.js",
    "spy": "./src/alias/spy.js",
    "spyb": "./src/alias/spyb.js",
    "spp": "./src/alias/spp.js",
    "spb": "./src/alias/spb.js",
    "sx": "./src/alias/sx.js"
  },
  "scripts": {
    "prebuild": "rm -rf bin/",
    "build": "tsc --project tsconfig.build.json",
    "postbuild": "cp package*.json .npmignore README.md LICENSE.md CHANGELOG.md bin && (cd bin && npm ci --omit=dev --ignore-scripts)",
    "pack": "npm pack bin/ --pack-destination=\"bin/\"",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --fix --ext .js,.ts",
    "test": "vitest --run --reporter verbose --no-threads",
    "test:w": "vitest --no-threads",
    "test:c": "vitest run --coverage --no-threads",
    "test:wc": "vitest watch --coverage --no-threads",
    "ts:check": "tsc --noEmit",
    "ts:trace": "tsc tsc --traceResolution"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deinsoftware/swpm.git"
  },
  "keywords": [
    "swpm",
    "npm",
    "yarn",
    "pnpm",
    "bun",
    "cli",
    "command-line",
    "package-manager",
    "javascript",
    "node",
    "nodejs"
  ],
  "author": "deinsoftware",
  "contributors": [
    "equiman"
  ],
  "sponsor": {
    "url": "https://github.com/sponsors/deinsoftware"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deinsoftware/swpm/issues"
  },
  "homepage": "https://github.com/deinsoftware/swpm#readme",
  "type": "module",
  "dependencies": {
    "chalk": "5.3.0",
    "command-exists": "1.2.9",
    "common-tags": "1.8.2",
    "find-up": "6.3.0",
    "prompts": "2.4.2",
    "semver": "7.5.4",
    "spinnies": "0.5.1",
    "update-notifier": "6.0.2",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@types/command-exists": "1.2.1",
    "@types/common-tags": "1.8.2",
    "@types/prompts": "2.4.4",
    "@types/semver": "7.5.2",
    "@types/spinnies": "0.5.0",
    "@types/update-notifier": "6.0.5",
    "@types/yargs": "17.0.24",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "@vitest/coverage-v8": "0.34.4",
    "eslint": "8.49.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-n": "16.1.0",
    "eslint-plugin-promise": "6.1.1",
    "type-fest": "4.3.1",
    "typescript": "5.2.2",
    "vitest": "0.34.4"
  },
  "volta": {
    "node": "18.18.0",
    "npm": "9.8.1"
  },
  "packageManager": "npm@9.8.1",
  "swpm": "npm"
}